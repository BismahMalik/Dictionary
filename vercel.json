{
  "version": 2,
  "builds": [
    
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "api/serverless.js" },
    { "src": "/(.*)", "dest": "public/$1" }
   
  ],
  "env": {
    "NODE_ENV": "production"
  }
}